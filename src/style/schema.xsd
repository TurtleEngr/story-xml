        <xs:complexType>
            <xs:attributeGroup ref='com-attr' />
            <xs:attribute name='date'
                          use='required' />
            <xs:attribute name='rev'
                          use='required' />
            <xs:attribute name='file' />
            <xs:attribute name='repository' />
            <xs:attribute name='root' />
            <xs:attribute name='source' />
            <xs:attribute name='svn-path' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='rev'>
        <xs:complexType mixed='true'>
            <xs:choice minOccurs='0'
                       maxOccurs='unbounded'>
                <xs:element ref='link' />
                <xs:element ref='img' />
                <xs:element ref='b' />
                <xs:element ref='i' />
                <xs:element ref='tt' />
            </xs:choice>
            <xs:attribute name='condition' />
            <xs:attribute name='id' />
            <xs:attribute name='lang' />
            <xs:attribute name='revision'>
                <xs:simpleType>
                    <xs:restriction base='xs:string'>
                        <xs:enumeration value='in-progress' />
                        <xs:enumeration value='draft' />
                        <xs:enumeration value='final' />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
 
            <xs:attribute name='revision-flag'
                          use='required'>
                <xs:simpleType>
                    <xs:restriction base='xs:string'>
                        <xs:enumeration value='changed' />
                        <xs:enumeration value='added' />
                        <xs:enumeration value='deleted' />
                        <xs:enumeration value='off' />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
 
        </xs:complexType>
    </xs:element>
 
    <xs:element name='s'>
        <xs:complexType mixed='true'>
            <xs:choice minOccurs='0'
                       maxOccurs='unbounded'>
                <xs:element ref='link' />
                <xs:element ref='img' />
                <xs:element ref='b' />
                <xs:element ref='i' />
                <xs:element ref='tt' />
                <xs:element ref='rev' />
            </xs:choice>
            <xs:attributeGroup ref='com-attr' />
            <xs:attribute name='who-ref' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='start-date'>
        <xs:complexType mixed='true'>
            <xs:attributeGroup ref='date-time' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='state'>
        <xs:complexType mixed='true'></xs:complexType>
    </xs:element>
 
    <xs:element name='story-dtd'>
        <xs:complexType>
            <xs:attribute name='version'
                          use='required'>
                <xs:simpleType>
                    <xs:restriction base='xs:string'>
                        <xs:enumeration value='1' />
                        <xs:enumeration value='2' />
                        <xs:enumeration value='3' />
                        <xs:enumeration value='4' />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
 
        </xs:complexType>
    </xs:element>
 
    <xs:element name='story-info'>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref='def-when' />
                <xs:choice minOccurs='0'
                           maxOccurs='unbounded'>
                    <xs:element ref='def-where' />
                    <xs:element ref='def-who' />
                    <xs:element ref='def-thread' />
                </xs:choice>
                <xs:element ref='ch-preface'
                            minOccurs='0' />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
 
    <xs:element name='street'>
        <xs:complexType mixed='true'></xs:complexType>
    </xs:element>
 
    <xs:element name='style-info'>
        <xs:complexType>
            <xs:sequence>
                <xs:choice minOccurs='0'
                           maxOccurs='unbounded'>
                    <xs:element ref='def-style' />
                    <xs:element ref='def-content' />
                    <xs:element ref='def-draft' />
                    <xs:element ref='def-print' />
                    <xs:element ref='def-base' />
                    <xs:element ref='def-img' />
                    <xs:element ref='def-link' />
                </xs:choice>
                <xs:element ref='meta-description' />
                <xs:element ref='file-prefix' />
            </xs:sequence>
            <xs:attribute name='style-ref'
                          use='required' />
            <xs:attribute name='thread-refs'
                          use='required' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='subtitle'>
        <xs:complexType mixed='true'>
            <xs:attributeGroup ref='com-attr' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='surname'>
        <xs:complexType mixed='true'>
            <xs:attributeGroup ref='com-attr' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='t'>
        <xs:complexType mixed='true'>
            <xs:choice minOccurs='0'
                       maxOccurs='unbounded'>
                <xs:element ref='link' />
                <xs:element ref='img' />
                <xs:element ref='b' />
                <xs:element ref='i' />
                <xs:element ref='tt' />
                <xs:element ref='rev' />
            </xs:choice>
            <xs:attributeGroup ref='com-attr' />
            <xs:attribute name='who-ref'
                          use='required' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='thread'>
        <xs:complexType>
            <xs:choice minOccurs='0'
                       maxOccurs='unbounded'>
                <xs:element ref='p' />
                <xs:element ref='pre' />
                <xs:element ref='s' />
                <xs:element ref='t' />
                <xs:element ref='quote' />
                <xs:element ref='br' />
            </xs:choice>
            <xs:attributeGroup ref='com-attr' />
            <xs:attribute name='ref'
                          use='required' />
            <xs:attribute name='who-refs'
                          use='required' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='title'>
        <xs:complexType mixed='true'>
            <xs:attributeGroup ref='com-attr' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='title-abbrev'>
        <xs:complexType mixed='true'>
            <xs:attributeGroup ref='com-attr' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='tt'>
        <xs:complexType mixed='true'>
            <xs:choice minOccurs='0'
                       maxOccurs='unbounded'>
                <xs:element ref='link' />
                <xs:element ref='img' />
                <xs:element ref='b' />
                <xs:element ref='i' />
                <xs:element ref='rev' />
            </xs:choice>
        </xs:complexType>
    </xs:element>
 
    <xs:element name='unit'>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref='title' />
                <xs:element ref='outline'
                            minOccurs='0' />
                <xs:element ref='thread'
                            maxOccurs='unbounded' />
            </xs:sequence>
            <xs:attributeGroup ref='com-attr-req' />
            <xs:attribute name='type'
                          use='required'>
                <xs:simpleType>
                    <xs:restriction base='xs:string'>
                        <xs:enumeration value='scene' />
                        <xs:enumeration value='sequel' />
                        <xs:enumeration value='other' />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
 
        </xs:complexType>
    </xs:element>
 
    <xs:element name='when'>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref='start-date' />
                <xs:element ref='end-date' />
                <xs:element ref='description'
                            minOccurs='0' />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
 
    <xs:element name='where'>
        <xs:complexType>
            <xs:choice minOccurs='0'
                       maxOccurs='unbounded'>
                <xs:element ref='para' />
                <xs:element ref='pre-fmt' />
                <xs:element ref='quote' />
                <xs:element ref='br' />
            </xs:choice>
            <xs:attributeGroup ref='ref-r' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='who'>
        <xs:complexType>
            <xs:choice minOccurs='0'
                       maxOccurs='unbounded'>
                <xs:element ref='para' />
                <xs:element ref='pre-fmt' />
                <xs:element ref='quote' />
                <xs:element ref='br' />
            </xs:choice>
            <xs:attributeGroup ref='ref-r' />
        </xs:complexType>
    </xs:element>
 
    <xs:element name='year'>
        <xs:complexType mixed='true'></xs:complexType>
    </xs:element>
 
</xs:schema>
