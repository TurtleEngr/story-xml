                extension-element-prefixes="date doc exsl str">
    <xsl:param name="gDraft"
               select="boolean(number('1'))" />
    <xsl:include href="com-param.xsl" />
    <xsl:include href="com-html.xsl" />
    <xsl:include href="com.xsl" />
    <xsl:include href="com-timeline.xsl" />
    <!-- ************************************************** -->
    <xsl:template match="/content">
        <xsl:apply-templates select="book" />
    </xsl:template>
 
    <!-- ************************************************** -->
    <xsl:template match="book">
        <xsl:call-template name="fTimeline" />
        <xsl:call-template name="fFmtProj" />
    </xsl:template>
 
    <!-- ******************** -->
    <xsl:template name="fFmtProj">
        <!--
Output the timeline as a OpenProj file (compatable with MS
Project 2003).
Units -> tasks
def-who -> resources
def-where -> resources
def-thread -> resources
-->
        <exsl:document method="xml"
                       href="{concat(@id, '-timeline.proj.xml')}">
            <xsl:apply-templates select="document('/tmp/timeline.xml')"
                                 mode="proj" />
        </exsl:document>
    </xsl:template>
 
    <!-- ******************** -->
    <xsl:template match="timeline"
                  mode="proj">
        <xsl:variable name="tDefWhen">
            <xsl:value-of select="concat(def-when/@year, '-', format-number(def-when/@month, '00'), '-', format-number(def-when/@day, '00'), 'T', format-number(def-when/@hour, '00'), ':', format-number(def-when/@min, '00'), ':00')" />
